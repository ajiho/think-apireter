{
  "name": "ajiho/think-apireter",
  "description": "Quickly return a unified API format for the thinkphp6 framework",
  "keywords": [
    "thinkphp6",
    "api",
    "response"
  ],
  "homepage": "https://gitee.com/ajiho/think-apireter",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ajiho\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "authors": [
    {
      "name": "ajiho",
      "email": "lujiahao@88.com",
      "homepage": "https://gitee.com/ajiho",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "topthink/framework": "^6.0",
    "ext-json": "*"
  },
  "extra": {
    "think": {
      "config": {
        "status": "src/status.php"
      }
    }
  }
}
